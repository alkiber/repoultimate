<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.Mirepor" name="[COLOR lime]Mi Repositorio[/COLOR] Repo" version="1.0.1" provider-name="Yo Mismo">
    <extension point="xbmc.addon.repository" name="Mi repositorio">
        <info>https://raw.githubusercontent.com/alkiber/repoultimate/master/Repositorio/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/alkiber/repoultimate/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/alkiber/master/Repositorio/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Repositorio para Mi</summary>
        <description></description>
        <disclaimer></disclaimer>
        <platform>all</platform>
    </extension>
</addon>

<addon id="org.harddevelop.kodi.tv"
	name="TV BOX"
	version="0.0.6"
	provider-name="bit" >
	<requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="script.module.parsedom" version="0.9.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<language>en</language>
		<summary lang="en">TV BOX channels</summary>
		<description lang="en">TV BOX channels client for browse and see online channels.</description>
		<description lang="es">Cliente de canales TV BOX para navegar y ver canales online.</description>
	</extension>
</addon>
</addons>
